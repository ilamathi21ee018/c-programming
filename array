#include <stdio.h>

int main()
{
  int arr[5]={11,12,13,14,15};
  for (int i=0;i<5;i++)
  {
      printf("%d",arr[i]);
  }
  
}
// revrse the element in an arry
#include <stdio.h>

int main()
{
  int n;
  scanf("%d",&n);
  int arr[n];
  for(int i=0;i>=n;i--)
  {
      scanf("%d",&arr[i]);
  }
  for (int i=0;i>=n;i--);
  {
      printf("%d",arr[1]);
  }
  
}
//forward
#include <stdio.h>

int main()
{
  int n;
  scanf("%d",&n);
  int arr[n];
  for(int i=0;i>=n;i--)
  {
      scanf("%d",&arr[i]);
  }
  for (int i=0;i>=n;i--);
  {
      printf("%d",arr[1]);
  }
  
}
// sum of the elements in arry
#include <stdio.h>

int main()
{
  int n;
  scanf("%d",&n);
  int arr[n];
  for(int i=0;i<n;i++)
  {
      scanf("%d",&arr[i]);
  }    
  for (int i=n-1;i>=0;i--)
  {
      printf("%d",arr[i]);
  }
   // sum of the elemens in array
   #include <stdio.h>

int main()
{
  int arr[5]={1,2,3,4,5};
  int sum=0;
  for(int i=0;i<5;i++)
  {
     sum=sum+arr[i];
  }
  printf("%d",sum);
}
// averge of an given array
#include <stdio.h>

int main()
{
  int arr[5]={1,2,3,4,5};
  int sum=0;
  int avg=3;
  for(int i=0;i<5;i++)
  {
     sum=sum+arr[i];
  }
  avg=sum/5;
  printf("%d",sum);
}

