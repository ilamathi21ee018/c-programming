#include <stdio.h>

int main()
{
  int arr[5]={11,12,13,14,15};
  printf("%d", arr [0]);
  
}
#include <stdio.h>

int main()
{
  int arr[5]={11,12,13,14,15};
  for (int i=0;i<5;i++)
  {
      printf("%d",arr[i]);
  }
  
}
// revrse the element in an arry
#include <stdio.h>

int main()
{
  int n;
  scanf("%d",&n);
  int arr[n];
  for(int i=0;i>=n;i--)
  {
      scanf("%d",&arr[i]);
  }
  for (int i=0;i>=n;i--);
  {
      printf("%d",arr[1]);
  }
  
}
